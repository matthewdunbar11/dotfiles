#!/bin/bash

# get sessions
tmux list-sessions -F "#S" |\
  # multiselect sessions to kill
  fzf -m --prompt="Select Sessions to Kill ($current_session):" |\
  while read session
  do
    [[ "$session" ]] || break # if none selected, exit loop
    tmux kill-session -t $session;
    echo "Killed Session: $session";
  done

